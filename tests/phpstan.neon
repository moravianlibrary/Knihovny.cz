parameters:
    phpVersion: 80100
    scanDirectories:
        - /var/www/knihovny-cz/module
    paths:
        - /var/www/knihovny-cz-extension/module
    tmpDir: %currentWorkingDirectory%/.phpstan_cache
    bootstrapFiles:
        - /var/www/knihovny-cz/tests/phpstan-constants.php
    level: 1
    reportUnmatchedIgnoredErrors: false
    parallel:
        processTimeout: 600.0
